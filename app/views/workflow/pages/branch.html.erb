<table class="index branches">
  <thead>
    <tr>
      <th><%= @model.t :name %></th>
      <th class="datetime"><%= @model.t :updated %></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
    <tr>
      <td class="name"><%= link_to item.name, show_path_with_route(item), target: "_blank" %></td>
      <td class="datetime"><%= item.updated.strftime("%Y/%m/%d %H:%M") %></td>
    </tr>
    <% end %>
  </tbody>
</table>
